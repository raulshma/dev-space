{
  "displayName": "Mira App",
  "name": "mira-app",
  "description": "An Electron application with React and TypeScript",
  "version": "1.0.0",
  "main": "./node_modules/.dev/main/index.mjs",
  "resources": "src/resources",
  "author": {
    "name": "Mira",
    "email": "mira@example.com"
  },
  "license": "MIT",
  "homepage": "https://electron-vite.org",
  "packageManager": "pnpm@10.0.0",
  "pnpm": {
    "onlyBuiltDependenciesFile": "./trusted-dependencies-scripts.json"
  },
  "scripts": {
    "start": "electron-vite preview",
    "predev": "run-s clean:dev",
    "dev": "cross-env NODE_ENV=development electron-vite dev --watch",
    "compile:app": "electron-vite build",
    "compile:packageJSON": "tsx ./src/lib/electron-app/release/modules/prebuild.ts",
    "prebuild": "run-s clean:dev compile:app compile:packageJSON",
    "build": "pnpm electron-builder",
    "postinstall": "run-s prebuild install:deps",
    "install:deps": "electron-builder install-app-deps",
    "make:release": "tsx ./src/lib/electron-app/release/modules/release.ts",
    "release": "electron-builder --publish always",
    "clean:dev": "rimraf ./node_modules/.dev",
    "lint": "biome check --no-errors-on-unmatched",
    "lint:fix": "biome check --write --no-errors-on-unmatched --assist-enabled=true",
    "typecheck": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@electron/rebuild": "^4.0.2",
    "@google/generative-ai": "^0.24.1",
    "@lydell/node-pty": "^1.1.0",
    "@tanstack/react-query": "^5.90.12",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-webgl": "^0.18.0",
    "@xterm/xterm": "^5.5.0",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "electron-router-dom": "^2.1.0",
    "electron-updater": "^6.6.2",
    "lucide-react": "^0.561.0",
    "openai": "^6.14.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tiktoken": "^1.0.22",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/ui": "^4.0.16",
    "code-inspector-plugin": "^1.3.3",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "fast-check": "^4.4.0",
    "happy-dom": "^20.0.11",
    "npm-run-all": "^4.1.5",
    "open": "^11.0.0",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "semver": "^7.7.3",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.16"
  }
}
